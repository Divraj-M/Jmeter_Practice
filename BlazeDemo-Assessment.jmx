            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion">
              <stringProp name="BeanShellAssertion.query">String response = prev.getResponseDataAsString();
String fromcity = vars.get(&quot;fromPort&quot;);
String tocity = vars.get(&quot;toPort&quot;);

if (!response.contains(&quot;Flights from &quot; + fromcity + &quot; to &quot; + tocity)) {
    Failure = true;
    FailureMessage = &quot;Flight message not found!&quot;;
} else {
    log.info(&quot;Flights from &quot; + fromcity + &quot; to &quot; + tocity);
}
</stringProp>
