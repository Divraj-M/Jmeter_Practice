            <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="BeanShell PreProcessor">
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">try
{
	String res=prev.getResponseDataAsString();
	String dep=res.split(&quot;name=\&quot;fromPort\&quot;&quot;)[1].split(&quot;&lt;/select&gt;&quot;)[0];
	String[] departure= dep.split(&quot;&lt;option value=\&quot;&quot;);

	String dest=res.split(&quot;name=\&quot;toPort\&quot;&quot;)[1].split(&quot;&lt;/select&gt;&quot;)[0];
	String[] destination=dest.split(&quot;&lt;option value=\&quot;&quot;);
	int len=departure.length;
	int len1=destination.length;

	String[] fromList=new String[len-1];
	String[] toList=new String[len1-1];

	for(int i=1;i&lt;len;i++)
	{
		fromList[i-1]=departure[i].split(&quot;\&quot;&quot;)[0];
	}
	for(int i=1;i&lt;len1;i++)
	{
   		toList[i-1]=destination[i].split(&quot;\&quot;&quot;)[0];
	}

	String fromPort=fromList[(int)(Math.random()*len-1)];
	String toPort=toList[(int)(Math.random()*len1-1)];
	vars.put(&quot;fromPort&quot;,fromPort);
	vars.put(&quot;toPort&quot;,toPort);
	log.info(&quot;From:&quot;+ fromPort +&quot;,To:&quot;+toPort);
}
catch(Exception e)
{
	vars.put(&quot;fromPort&quot;,&quot;Paris&quot;);
	vars.put(&quot;toPort&quot;,&quot;Berlin&quot;);
	log.warn(&quot;The try block have some error&quot;+e.toString());
}
</stringProp>
            </BeanShellPreProcessor>
