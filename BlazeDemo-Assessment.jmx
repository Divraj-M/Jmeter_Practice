            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">String res=prev.getResponseDataAsString();
String regex=&quot;&lt;input type=\&quot;hidden\&quot; value=\&quot;(.*?)\&quot; name=\&quot;flight\&quot;&gt;&quot;;
String regex1=&quot;&lt;h3&gt;(.*?)&lt;/h3&gt;&quot;;

java.util.regex.Pattern pattern=java.util.regex.Pattern.compile(regex);
java.util.regex.Pattern pattern1=java.util.regex.Pattern.compile(regex1);

java.util.regex.Matcher matcher=pattern.matcher(res);
java.util.regex.Matcher matcher1=pattern1.matcher(res);

if(matcher.find()) 
{
    String flightId=matcher.group(1);
    String flightname=&quot;&quot;;
    if(matcher1.find()) 
    {
        flightname=matcher1.group(1);
    }
    log.info(&quot;The flight name is &quot;+flightname);
    log.info(&quot;First flight ID is &quot;+flightId);
} 
else 
{
    log.error(&quot;Flight ID not found&quot;);
}
</stringProp>
