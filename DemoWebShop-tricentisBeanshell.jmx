            <stringProp name="BeanShellSampler.query">import org.jsoup.*;
import org.jsoup.nodes.*;
import org.jsoup.select.*;

int maxRetries = 3;
int retryCount = 0;
boolean success = false;
String html = &quot;&quot;;

while (retryCount &lt; maxRetries &amp;&amp; !success) {
    try {
        html = prev.getResponseDataAsString();
        Document doc = Jsoup.parse(html);

        Elements cartRows = doc.select(&quot;tr.cart-item-row&quot;);

        if (cartRows.size() &gt; 0) {
            success = true;
            log.info(&quot;✅ Found &quot; + cartRows.size() + &quot; items in cart.&quot;);
            int index = 1;
            for (Element row : cartRows) {
                String name = row.select(&quot;.product-name&quot;).text();
                String productUrl = row.select(&quot;.product-name&quot;).attr(&quot;href&quot;);
                String qty = row.select(&quot;input.qty-input&quot;).val();
                String price = row.select(&quot;.product-unit-price&quot;).text();
                String subtotal = row.select(&quot;.product-subtotal&quot;).text();

                log.info(&quot;🛒 Item &quot; + index + &quot;:&quot;);
                log.info(&quot;🔹 Name     : &quot; + name);
                log.info(&quot;🔹 URL      : &quot; + productUrl);
                log.info(&quot;🔹 Qty      : &quot; + qty);
                log.info(&quot;🔹 Price    : &quot; + price);
                log.info(&quot;🔹 Subtotal : &quot; + subtotal);
                log.info(&quot;--------------------------------------------------&quot;);

                index++;
            }
        } else {
            retryCount++;
            log.warn(&quot;⚠️ Attempt &quot; + retryCount + &quot; failed: No cart items found.&quot;);
            Thread.sleep(1000); // Wait 1 second before retry
        }

    } catch (Exception e) {
        retryCount++;
        log.error(&quot;❌ Exception at attempt &quot; + retryCount + &quot;: &quot; + e.getMessage());
        Thread.sleep(1000); // Wait before next retry
    }
}

if (!success) {
    log.error(&quot;❌ Failed to extract cart items after &quot; + maxRetries + &quot; attempts.&quot;);
}
</stringProp>
