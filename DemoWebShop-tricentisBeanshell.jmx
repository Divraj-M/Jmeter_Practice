              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">import org.apache.jmeter.engine.StopTestException;

// Email list with a few invalid entries
String[] emails = {
    &quot;Maggie28@gmail.com&quot;,
    &quot;Amelia_Metz@gmail.com&quot;,
    &quot;invalidEmail@@gmail&quot;,       // Invalid
    &quot;Brandi.Bradtke1@gmail.com&quot;,
    &quot;Nicklaus33@gmail.com&quot;,
    &quot;noatsymbol.com&quot;,            // Invalid
    &quot;Joesph_Erdman@gmail.com&quot;
};

// Randomly select an email
int randomIndex = (int)(Math.random() * emails.length);
String selectedEmail = emails[randomIndex];

// Email validation
boolean isValidEmail(String email) {
    return email.matches(&quot;^[A-Za-z0-9+_.-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,6}$&quot;);
}

// Check validity
if (!isValidEmail(selectedEmail)) {
    log.error(&quot;❌ Invalid Email Selected: &quot; + selectedEmail);

    // Stop the entire test
    throw new StopTestException(&quot;❌ Stopping test due to invalid email: &quot; + selectedEmail);
}

// Save to JMeter variable
vars.put(&quot;selectedEmail&quot;, selectedEmail);
log.info(&quot;✅ Valid Email Selected: &quot; + selectedEmail);
</stringProp>
