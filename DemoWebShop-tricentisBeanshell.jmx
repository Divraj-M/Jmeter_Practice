            <stringProp name="BeanShellSampler.query">import org.json.JSONObject;

// Get response data
String response = prev.getResponseDataAsString();
JSONObject json = null;

int retryCount = 0;
boolean success = false;
String message = &quot;&quot;;

try {
    while (retryCount &lt; 3) {
        json = new JSONObject(response);
        success = json.getBoolean(&quot;success&quot;);

        if (success) {
            message = json.getString(&quot;message&quot;);

            // Optional HTML unescaping
            message = message.replaceAll(&quot;\\\\u003c&quot;, &quot;&lt;&quot;).replaceAll(&quot;\\\\u003e&quot;, &quot;&gt;&quot;);

            vars.put(&quot;add_status&quot;, message);
            log.info(&quot;✅ Product Added Message: &quot; + message);

            // Handle special messages
            if (message.contains(&quot;already in your shopping cart&quot;)) {
                vars.put(&quot;already_in_cart&quot;, &quot;true&quot;);
                log.warn(&quot;⚠️ Product already added to cart.&quot;);
            } else {
                vars.put(&quot;already_in_cart&quot;, &quot;false&quot;);
                log.info(&quot;🆕 Fresh product added to cart.&quot;);
            }

            // Extract advanced fields if present
            if (json.has(&quot;cartCount&quot;)) {
                vars.put(&quot;cart_count&quot;, String.valueOf(json.getInt(&quot;cartCount&quot;)));
                log.info(&quot;🛒 Cart Count: &quot; + json.getInt(&quot;cartCount&quot;));
            }

            if (json.has(&quot;productId&quot;)) {
                vars.put(&quot;product_id&quot;, String.valueOf(json.getInt(&quot;productId&quot;)));
                log.info(&quot;🆔 Product ID: &quot; + json.getInt(&quot;productId&quot;));
            }

            break; // success so exit retry loop
        } else {
            retryCount++;
            log.warn(&quot;🔁 Retry attempt &quot; + retryCount + &quot;: &apos;success&apos; was false&quot;);
            Thread.sleep(1000); // wait 1 second before retry
            response = prev.getResponseDataAsString(); // refresh (if sampler re-ran)
        }
    }

    if (!success) {
        log.error(&quot;❌ Final retry failed. No success response.&quot;);
        vars.put(&quot;add_status&quot;, &quot;FAILED&quot;);
    }

} catch (Exception e) {
    log.error(&quot;❌ JSON Parse Error: &quot; + e.getMessage());
    vars.put(&quot;add_status&quot;, &quot;ERROR&quot;);
}
</stringProp>
