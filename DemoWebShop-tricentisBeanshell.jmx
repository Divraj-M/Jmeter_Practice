              <stringProp name="script">// Step 1: Define country IDs
String[] countryIds = { &quot;41&quot;, &quot;1&quot;, &quot;2&quot;, &quot;78&quot;, &quot;54&quot; };  // 41=India, 1=USA, etc.

// Step 2: Random country selection
int randIndex = (int) (Math.random() * countryIds.length);
String selectedCountryId = countryIds[randIndex];
vars.put(&quot;countryId&quot;, selectedCountryId);

// Step 3: Generate pincode based on selected country
int pin;

if (selectedCountryId.equals(&quot;41&quot;)) {           // India
    pin = 100000 + (int)(Math.random() * 900000);
} else if (selectedCountryId.equals(&quot;1&quot;)) {     // USA
    pin = 10000 + (int)(Math.random() * 90000);
} else if (selectedCountryId.equals(&quot;2&quot;)) {     // UK
    pin = 1000 + (int)(Math.random() * 9000);
} else if (selectedCountryId.equals(&quot;78&quot;)) {    // Canada
    pin = 100000 + (int)(Math.random() * 900000);
} else if (selectedCountryId.equals(&quot;54&quot;)) {    // Germany
    pin = 10000 + (int)(Math.random() * 90000);
} else {
    pin = 999999;
}
vars.put(&quot;pincode&quot;, String.valueOf(pin));

// Step 4: Generate random product quantity (1 to 10)
int qty = 1 + (int)(Math.random() * 10);
vars.put(&quot;productQty&quot;, String.valueOf(qty));

// Log for debugging
log.info(&quot;🌍 Selected Country ID: &quot; + selectedCountryId);
log.info(&quot;📦 Generated Pincode: &quot; + pin);
log.info(&quot;🛒 Selected Product Quantity: &quot; + qty);
