              <stringProp name="script">import org.jsoup.Jsoup;
import org.jsoup.nodes.Document;
import org.jsoup.nodes.Element;

try {
    // Get the previous response
    String html = prev.getResponseDataAsString();

    // Parse HTML using Jsoup
    Document doc = Jsoup.parse(html);

    // Find the first button with onclick containing &apos;addproducttocart_catalog&apos;
    Element button = doc.selectFirst(&quot;input.button-2.product-box-add-to-cart-button[onclick*=addproducttocart_catalog]&quot;);

    if (button != null) {
        String onclickValue = button.attr(&quot;onclick&quot;); // Example: AjaxCart.addproducttocart_catalog(&apos;/addproducttocart/catalog/45/1/1&apos;);

        // Extract path using regex
        java.util.regex.Matcher matcher = java.util.regex.Pattern.compile(&quot;&apos;(.*?)&apos;&quot;).matcher(onclickValue);
        if (matcher.find()) {
            String cartPath = matcher.group(1).trim(); // Clean any extra spaces
            vars.put(&quot;cartPath&quot;, cartPath); // Save to JMeter variable
            log.info(&quot;✅ Extracted cart path: &quot; + cartPath);
        } else {
            log.warn(&quot;⚠️ No matching path found in onclick attribute.&quot;);
        }
    } else {
        log.warn(&quot;⚠️ No matching &apos;Add to cart&apos; button found.&quot;);
    }

} catch (Exception e) {
    log.error(&quot;🔥 Exception while extracting cart path: &quot; + e.getMessage(), e);
    throw new RuntimeException(&quot;🔥 Failed to extract cart path due to error: &quot; + e.getMessage());
}
