            <BeanShellAssertion guiclass="BeanShellAssertionGui" testclass="BeanShellAssertion" testname="BeanShell Assertion">
              <stringProp name="BeanShellAssertion.query">import java.util.regex.Pattern;
import java.util.regex.Matcher;

// Get the email from JMeter variable
String email = vars.get(&quot;selectedEmail&quot;);

// Email regex pattern (basic standard)
String regex = &quot;^[A-Za-z0-9+_.-]+@[A-Za-z0-9.-]+\\.[A-Za-z]{2,6}$&quot;;

// Compile pattern
Pattern pattern = Pattern.compile(regex);
Matcher matcher = pattern.matcher(email);

// If email is invalid — fail the assertion
if (!matcher.matches()) {
    Failure = true;
    FailureMessage = &quot;❌ Assertion Failed: Invalid Email Format - &quot; + email;
    log.error(FailureMessage);
} else {
