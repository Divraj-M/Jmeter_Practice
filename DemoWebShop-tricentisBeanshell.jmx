// Define general success keywords
String[] expectedKeywords = { &quot;The product has been added&quot;, &quot;shopping cart&quot;, &quot;cart-qty&quot;, &quot;/cart&quot; };

boolean found = false;

// First, check general success message
for (int i = 0; i &lt; expectedKeywords.length; i++) {
    if (response.contains(expectedKeywords[i])) {
        found = true;
        break;
    }
}

// Second, check if expected product is mentioned as substring
if (!found &amp;&amp; expectedProduct != null &amp;&amp; response.contains(expectedProduct)) {
    found = true;
    log.info(&quot;✅ Substring match for product name found: &quot; + expectedProduct);
}

if (!found) {
    Failure = true;
    FailureMessage = &quot;❌ Expected product or success message not found in response.\nExpected Substring: &quot; + expectedProduct;
} else {
    log.info(&quot;✅ Product added and verified in response.&quot;);
}
</stringProp>
