              <stringProp name="script">// Step 1: Define country IDs
String[] countryIds = { &quot;41&quot;, &quot;1&quot;, &quot;2&quot;, &quot;78&quot;, &quot;54&quot; };  // 41=India, 1=USA, etc.

// Step 2: Random country selection
int randIndex = (int) (Math.random() * countryIds.length);
String selectedCountryId = countryIds[randIndex];
vars.put(&quot;countryId&quot;, selectedCountryId);

// Step 3: Generate pincode based on selected country
int pin;

if (selectedCountryId.equals(&quot;41&quot;)) {           // India
    pin = 100000 + (int)(Math.random() * 900000);
